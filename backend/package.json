{
  "name": "node_ts_template",
  "version": "1.0.0",
  "description": "basic template for node ts backend applications",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/server.ts",
    "start": "node ./dist/server.js",
    "prod": "npm run build && npm start",
    "deploy": "gcloud app deploy app.yaml --stop-previous-version --project eon-unlimited-42"
  },
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/mongodb": "^3.0.9",
    "@types/mongoose": "^5.0.7",
    "bcrypt": "^4.0.1",
    "body-parser": "*",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongojs": "^3.1.0",
    "mongoose": "^5.0.11",
    "request": "^2.88.0",
    "socket.io": "^2.3.0",
    "typescript": "^3.7.5",
    "uuid-token-generator": "^1.0.0",
    "winston": "^3.2.1"
  },
  "author": "Armin Windlin",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "babel-polyfill": "^6.26.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1"
  }
}
